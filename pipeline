peline {
	agent any

	stages { 
		stage ('Compile Stage') {

			steps {
				withMaven(maven : '/usr/share/maven') {
					sh 'mvn clean compile'
				}
			}
		}
		stages {'Testing Stage') {
			steps {
				withMaven(maven : '/usr/share/maven') {
					sh 'mvn test'
				}
			}
		}


		stage ('Deployment Stage') {

			steps {
				withMaven(maven : '/usr/share/maven') {
					sh 'mvn deploy'
				}
			}
		}
	}
}	
